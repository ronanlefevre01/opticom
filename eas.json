{
  "cli": {
    "version": ">= 16.15.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_SERVER_BASE": "https://opticom-sms-server.onrender.com"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_SERVER_BASE": "https://opticom-sms-server.onrender.com"
      }
    },
    "production": {
      "autoIncrement": true,
      "distribution": "store",
      "ios": { "simulator": false },
      "env": {
        "EXPO_PUBLIC_SERVER_BASE": "https://opticom-sms-server.onrender.com"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
